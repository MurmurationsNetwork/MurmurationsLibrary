{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "https://test-cdn.murmurations.network/schemas/test_schema-v2.3.9.json",
  "title": "Test Schema",
  "description": "Just for testing.",
  "type": "object",
  "properties": {
    "linked_schemas": {
      "$ref": "../fields/linked_schemas.json"
    },
    "people": {
      "title": "People",
      "description": "A list of people and information about each of them.",
      "type": "array",
      "items": [
        {
          "title": "Name",
          "description": "The name of the person.",
          "type": "string"
        },
        {
          "title": "Age",
          "description": "The age of the person.",
          "type": "number"
        },
        {
          "title": "Gender",
          "description": "The gender of the person.",
          "enum": [
            "male",
            "female"
          ]
        },
        {
          "title": "Address",
          "description": "The person's home address.",
          "type": "object",
          "properties": {
            "number": {
              "title": "Street Number",
              "description": "The street number.",
              "type": "number"
            },
            "street": {
              "title": "Street Name",
              "description": "The street name.",
              "type": "string"
            },
            "planet": {
              "title": "Planet",
              "description": "The planet the person lives on.",
              "enum": [
                "Earth",
                "Venus",
                "Mars"
              ]
            }
          }
        }
      ]
    }
  },
  "required": [
    "linked_schemas",
    "people"
  ],
  "metadata": {
    "creator": {
      "name": "Murmurations Network",
      "url": "https://murmurations.network/"
    },
    "schema": {
      "name": "test_schema-v2.3.9"
    }
  }
}
