{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "https://test-cdn.murmurations.network/schemas/test_schema-v2.3.8.json",
  "title": "Test Schema",
  "description": "Just for testing.",
  "type": "object",
  "properties": {
    "linked_schemas": {
      "$ref": "../fields/linked_schemas.json"
    },
    "store": {
      "title": "Store & Inventory",
      "description": "A store with its inventory list.",
      "type": "object",
      "properties": {
        "name": {
          "title": "Store Name",
          "type": "string",
          "description": "The name of the store."
        },
        "inventory": {
          "title": "Inventory",
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "item": {
                "title": "Item",
                "description": "The item for sale.",
                "type": "string"
              },
              "amount": {
                "title": "Amount",
                "description": "The amount of the item.",
                "type": "number"
              }
            },
            "required": [
              "item",
              "amount"
            ]
          }
        },
        "city": {
          "title": "City",
          "description": "The city where the store is located.",
          "type": "string"
        }
      },
      "required": [
        "name",
        "city"
      ]
    }
  },
  "required": [
    "linked_schemas",
    "store"
  ],
  "metadata": {
    "creator": {
      "name": "Murmurations Network",
      "url": "https://murmurations.network/"
    },
    "schema": {
      "name": "test_schema-v2.3.8"
    }
  }
}
